<%= month_calendar events: slots do |date, slots| %>
<%= date.day %>

  <% slots.each do |slot| %>
    <br>
    <%= link_to slot.start_format_2, new_meeting_path, class: "btn btn-primary" %>
    <%= link_to "X", user_slot_path(@user, slot), method: :delete, class: "btn btn-danger"  %>

  <% end %>
<% end %>

